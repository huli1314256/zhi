#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.4.5/node_modules/vuepress/bin/node_modules:/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.4.5/node_modules/vuepress/node_modules:/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.4.5/node_modules:/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.4.5/node_modules/vuepress/bin/node_modules:/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.4.5/node_modules/vuepress/node_modules:/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.0_@vuepress+client@2.0.0-rc.0_vue@3.4.5/node_modules:/mnt/c/HHHHHHHH/xiangmu/my-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress.js" "$@"
fi
